<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet id="14b" author="adamut.cosmin">

        <createTable tableName="ZTH2017_ADDRESS_COSMIN">
            <column name="adr_id" type="NUMBER(6)">
                <constraints primaryKey="true"/>
            </column>
            <column name="street" type="VARCHAR2(50)"/>
            <column name="no" type="NUMBER(6)"/>
            <column name="city" type="VARCHAR2(30)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>


</databaseChangeLog>

